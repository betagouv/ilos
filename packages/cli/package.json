  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/ilos.git"
  },
  "scripts": {
    "copy-static": "cp -R ./src/stubs ./dist/",
    "build": "tsc && yarn copy-static",
    "test": "mocha --config ../../.mocharc.js \"src/**/*.spec.ts\"",
    "test:integration": "mocha tests/**/*.spec.ts",
    "coverage-ci": "nyc --all --reporter=lcov mocha src/*.spec.ts src/**/*.spec.ts tests/**/*.spec.ts",
    "coverage": "nyc --all --reporter=text mocha src/*.spec.ts src/**/*.spec.ts tests/**/*.spec.ts",
    "watch-test": "yarn test -- --watchAll",
    "watch": "tsc -w",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/sinon": "^7.5.1",
    "sinon": "^8.1.1"
  },
  "dependencies": {
    "commander": "^4.1.1",
    "@ilos/common": "^0.4.1",
    "@ilos/core": "^0.4.1"
  }
}
